  <xacro:property name="M_PI" value="3.1415926535897931"/>
  <xacro:property name="COL_K" value="1.2"/>
  <!-- =============== Link & Joint =============== -->
  <joint name="arm/root" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <parent link="base"/>
    <child link="arm/link_0"/>
  </joint>

  <link name="arm/link_0">
    <visual>
      <geometry>
        <mesh filename="package://torobo_resources/TWDY-R-C-001-7/meshes/link0.dae" scale="0.01 0.01 0.01"/>
      </geometry>
      <origin xyz="0 0 0"/>
